{
  "name": "@cloudscape-design/jest-preset",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudscape-design/jest-preset.git"
  },
  "homepage": "https://cloudscape.design",
  "main": "./jest-preset.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "prebuild": "rm -rf lib && mkdir lib",
    "build": "echo 'Nothing to build'",
    "postbuild": "cp *.js README.md LICENSE NOTICE package.json lib",
    "lint": "eslint .",
    "pretest": "node -e \"require('fs').mkdirSync('coverage', { recursive: true })\"",
    "test": "node --test --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=coverage/lcov.info",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "babel-jest": ">=24",
    "jest": ">=24"
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-header": "3.1.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^50.0.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.10",
    "prettier": "^3.2.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "tap": {
    "coverage-report": [
      "lcov",
      "text",
      "json"
    ]
  }
}
